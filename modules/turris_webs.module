#!modules/module.sh

help="
$(gettext "test whether the turris webs are available")
"

test_web() {
	local url="$1"
	shift
	echo_sec "$url"
	curl --location --verbose "$url" https://api.turris.cz/ > /dev/null
}

run () {
	test_web "https://repo.turris.cz"
	test_web "https://www.turris.cz"
}

# vim: ft=sh
