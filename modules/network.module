#!modules/module.sh

help="
$(gettext "prints uci network and wireless settings, current ip addresses and routes")
"

run () {
	echo_sec "Uci wireless"
	uci show wireless | grep -v ".key="

	echo_sec "Uci network"
	uci show network

	echo_sec "IPv4"
	ip -4 address
	ip -4 route

	echo_sec "IPv6"
	ip -6 address
	ip -6 route
}

# vim: ft=sh
