#!modules/module.sh

help="
$(gettext "Prints UCI network and wireless settings, current IP addresses, and routes.")
"

run () {
	echo_sec "Uci wireless"
	uci show wireless | grep -v ".key="

	echo_sec "Uci network"
	uci show network

	echo_sec "Links"
	ip -d link


	echo_sec "IPv4"
	ip -d -4 address
	ip -d -4 route

	echo_sec "IPv6"
	ip -d -6 address
	ip -d -6 route
}

# vim: ft=sh
